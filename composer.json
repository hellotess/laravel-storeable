{
    "name": "hellotess/laravel-storelevel",
    "description": "A trait to make an Eloquent model hold translations",
    "homepage": "https://github.com/spatie/laravel-translatable",
    "license": "MIT",
    "authors": [
        {
            "name": "Kilian Hann",
            "email": "kilian.hann@hellotess.com",
            "homepage": "https://hellotess.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/database": "^9.0",
        "illuminate/support": "^9.0",
        "spatie/laravel-package-tools": "^1.11"
    },
    "require-dev": {
        "mockery/mockery": "^1.4",
        "orchestra/testbench": "^7.0",
        "pestphp/pest": "^1.20"
    },
    "autoload": {
        "psr-4": {
            "Hellotess\\Storable\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Hellotess\\Storable\\Test\\": "tests"
        }
    },
    "scripts": {
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes",
        "test": "vendor/bin/pest"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        },
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Spatie\\Translatable\\TranslatableServiceProvider"
            ]
        },
        "aliases": {
            "Translatable": "Spatie\\Translatable\\Facades\\Translatable"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
